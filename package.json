    "typescript": "^5.7.2"
  },
  "scripts": {
    "lint": "biome lint ./",
    "lint:fix": "biome lint --write ./",
    "format": "biome format ./",
    "format:fix": "biome format --write ./",
    "typecheck": "tsc --noEmit",
    "prebuild": "npm run typecheck",
    "build": "tsup"
  }
}
